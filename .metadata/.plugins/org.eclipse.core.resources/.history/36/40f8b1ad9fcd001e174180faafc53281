package poo2Abstracao;

public class Carro {
	//atributos
	int ano;
	String cor;
	
	//metodos
	void ligar() {
		System.out.println("Engine ON.........");
		
	}
	
	void desligar() {
		System.out.println("Engine OFF.........");
		
	}
	
	
	void acelerar() {
		System.out.println("vrummmmmmmmmmm...");
		
	}
	
	
	

}
