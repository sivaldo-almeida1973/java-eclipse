package contas;

public class Conta {
	//atributos
	public String cliente;
	public double saldo;
	
	//construtor(tem que ter mesmo nome da classe)
	public Conta() {
		System.out.println("Agencia 0261");		
	}
	
	//metodos(funcao)
	
	public void exibirSaldo() {
		System.out.println("Saldo: R$ " + saldo);
	}
}
