package construtor;

public class Camaro {

	public static void main(String[] args) {
		//
		Carro camaro = new Carro();
		camaro.ano = 2010;
		camaro.cor = "Amarelo";
		camaro.ligar();
		camaro.acelerar();
		camaro.desligar();
		System.out.println("Carro: Camaro");
		System.out.println("Ano; "+ camaro.ano);
		System.out.println("Cor:"+ camaro.cor );

	}

}
