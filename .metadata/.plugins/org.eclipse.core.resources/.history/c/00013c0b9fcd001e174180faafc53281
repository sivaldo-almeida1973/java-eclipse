package poo;

public class Carro {
	int ano;
	String cor;
	
	//metodos
	void ligar() {
		System.out.println("engine ON.......................");
	}
	
	void desligar() {
		System.out.println("engine OFF.......................");
	}
	
	void acelerar() {
		System.out.println("vrummmmmmmmmmmmmm..............");
	}

}
