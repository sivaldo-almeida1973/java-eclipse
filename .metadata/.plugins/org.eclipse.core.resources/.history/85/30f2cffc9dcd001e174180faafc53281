package poo2;

public class Gol {

	public static void main(String[] args) {
		// inicia chmando a classe Carro e depois o nome do obj
		Carro gol = new Carro();
		gol.ano = 89;
		gol.cor = "Vermelho";
		System.out.println("Carro: Gol");
		System.out.println("Ano: " + gol.ano);
		System.out.println("Ano: " + gol.cor);
		gol.ligar();
		gol.acelerar();
		
		

	}

}
