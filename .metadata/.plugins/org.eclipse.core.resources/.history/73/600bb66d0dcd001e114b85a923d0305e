package cursoJava;

public class LacosRepeticao {

	public static void main(String[] args) {
		
		System.out.println("");
		System.out.println("Estruturas de repetições");
		System.out.println("");
		System.out.println("Exemplos 5 - Uso do 'for' ");
		for (int j = 10; j > 0; j--) {
			System.out.println("hello");
		}
	
		
		
	
	
		
	}

}
