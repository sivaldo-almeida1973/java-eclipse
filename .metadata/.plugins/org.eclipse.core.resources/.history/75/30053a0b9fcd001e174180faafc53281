package poo;

public class Ferrari {

	public static void main(String[] args) {
		// criar ojb ferrari
		Carro ferrari = new Carro();
		ferrari.ano = 2024;
		ferrari.cor = "Vermelho";
		System.out.println("Carro: Ferrari");
		System.out.println("Ano:" + ferrari.ano);
		System.out.println("Cor:" + ferrari.cor);
		ferrari.ligar();
		ferrari.acelerar();
		
		
		

	}

}
